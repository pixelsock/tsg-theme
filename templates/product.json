    "466c6b5e-c6cc-41a8-9869-b386b7b1271e": {
      "type": "custom-liquid",
      "settings": {
        "separate_section_with_border": true,
        "remove_vertical_spacing": true,
        "remove_horizontal_spacing": true,
        "liquid": "<script type=\"text\/javascript\">\n  var _learnq = _learnq || [];\n  var addToCart = function() {\n    _learnq.push(['track', 'Added to Cart', item]);\n  };\n\n  \/\/ Helper function to filter elements based on inner text\n  function filterElementsByInnerText(elements, text) {\n    var filteredElements = [];\n    elements.forEach(function(element) {\n      if (element.innerText.trim() === text) {\n        filteredElements.push(element);\n      }\n    });\n    return filteredElements;\n  }\n\n  \/\/ Find all the buttons with the type attribute set to \"submit\"\n  var allButtons = document.querySelectorAll('button[type=\"submit\"]');\n\n  \/\/ Filter the buttons based on inner text \"ADD TO CART\"\n  var buttons = filterElementsByInnerText(allButtons, 'ADD TO CART');\n\n  \/\/ Loop through the selected buttons to add the event listener\n  buttons.forEach(function(button) {\n    button.addEventListener('click', addToCart, false);\n  });\n<\/script>",
        "text_alignment": "start"
      }
    }
